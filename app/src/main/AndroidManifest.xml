<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.carolynbicycleshop.bikeshop42">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BikeShop42">
        <receiver
            android:name=".All.UI.MyReceiver"
            android:enabled="true"
            android:exported="true"></receiver>

        <activity android:name=".All.UI.PartsList" />
        <activity
            android:name=".All.UI.ProductList"
            android:label="@string/myproducts" />
        <activity android:name=".All.UI.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>